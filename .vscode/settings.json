// ユーザ設定を上書き
{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  // エディターで制御文字を表示する必要があるかどうかを制御します
  "editor.renderControlCharacters": true,
  "editor.renderIndentGuides": true,
  // フォント ファミリを制御します。
  "editor.fontFamily": "'MS Gothic'",
  // 改行コードを設定
  "files.eol": "\n",
  // エディターで空白文字を表示する方法を制御します。'none'、'boundary' および 'all' が使用可能です。'boundary' オプションでは、単語間の単一スペースは表示されません。
  "editor.renderWhitespace": "all",
  // Git Bash
  "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
  // tslintに関する設定
  "tslint.enable": true,
  "tslint.configFile": "./tslint.json",
  "typescript.tsdk": "./node_modules/typescript/lib"
  // 有効な場合、ファイルを開くときに文字セット エンコードを推測します
  //,"files.autoGuessEncoding": false,
  //postcssをとりあえずless記法とする。
  ,
  "files.associations": {
    "*.pcss": "less"
  },
  "[elm]": {
    "editor.formatOnSave": true,
    "editor.formatOnSaveTimeout": 1500
  },
  "elm.compiler": "..\\garden\\node_modules\\.bin\\elm",
  "elm.makeCommand": "..\\garden\\node_modules\\.bin\\elm-make",
  "elm.formatCommand": "..\\garden\\node_modules\\.bin\\elm-format",
  //,"elm.formatCommand": "D:\\develop\\develop\\node_modules\\elm-test\\bin\\elm-test"
  // VSCODEのエクスプローラー上で非表示
  "files.exclude": {
    "elm-stuff": true,
    "node_modules": true,
    "LICENSE": true,
    "**/.DS_Store": true,
    "*console.log": true,
  }
}